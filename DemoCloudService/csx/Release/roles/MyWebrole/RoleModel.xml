<?xml version="1.0" encoding="utf-8"?>
<RoleModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="MyWebrole" version="2.1.6493.1" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <Startup>
    <Task commandLine="DiagnosticsAgent.exe" taskType="background" relativePath="plugins\Diagnostics">
      <Environment>
        <Variable name="RdModuleName" value="Diagnostics" />
        <Variable name="WaRoleType" value="IISWebRole" />
      </Environment>
    </Task>
    <Task commandLine="DiagnosticsAgent.exe /blockStartup" relativePath="plugins\Diagnostics">
      <Environment>
        <Variable name="RdModuleName" value="Diagnostics" />
        <Variable name="WaRoleType" value="IISWebRole" />
      </Environment>
    </Task>
  </Startup>
  <Runtime>
    <EntryPoint>
      <NetFxEntryPoint assemblyName="MyWebrole.dll" targetFrameworkVersion="v4.0" />
    </EntryPoint>
  </Runtime>
  <ConfigurationSettings>
    <Setting name="ConnectionString" nameSpace="Microsoft.WindowsAzure.Plugins.Diagnostics" />
  </ConfigurationSettings>
  <Properties>
    <Property name="TargetFrameworkVersion" value="v4.5" />
    <Property name="RoleType" value="Web" />
    <Property name="RoleName" value="MyWebrole" />
    <Property name="ProjectName" value="MyWebrole" />
    <Property name="Configuration" value="Release" />
    <Property name="Platform" value="AnyCPU" />
    <Property name="RoleTargetFramework" value="v4.5" />
    <Property name="OutputDir" value="C:\taskmanagement\DemoCloudService\obj\Release\MyWebrole\" />
    <Property name="EntryPoint" value="MyWebrole.dll" />
    <Property name="SourceDir" value="C:\taskmanagement\MyWebrole\" />
    <Property name="SitePhysicalPath" value="C:\taskmanagement\DemoCloudService\obj\Release\MyWebrole\" />
    <Property name="TargetRuntimeVersion" value="v4.0" />
  </Properties>
  <Sites>
    <Site name="Web" physicalDirectory="C:\taskmanagement\DemoCloudService\obj\Release\MyWebrole\">
      <Bindings>
        <Binding name="Endpoint1" endpointName="Endpoint1" />
      </Bindings>
    </Site>
  </Sites>
</RoleModel>